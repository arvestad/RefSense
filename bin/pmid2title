#! /usr/bin/perl -w

use lib "/home/compbio/arve/src/biblio/cgi-bin";
use PubMed;
use PMCache;

my @pmids = ();
while (<>) {
  if (/^\s*(\d+)/) {
    push @pmids, $1;
  }
}
if (scalar(@pmids) == 0) {
  print STDERR "No PubMed identifiers.\n";
  exit(0);
}

my $art_h = cached_get_pmids(\@pmids);
foreach my $id (keys %$art_h) {
  printf("%9d   %s\n", $id, $art_h->{$id}{'TI'});
}
